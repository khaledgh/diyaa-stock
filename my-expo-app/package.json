{
  "name": "my-expo-app",
  "version": "1.0.0",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-bluetooth-classic",
          "react-native-thermal-printer"
        ]
      }
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.1.10",
    "arabic-reshaper": "^1.1.0",
    "axios": "^1.7.9",
    "buffer": "^6.0.3",
    "expo": "54.0.22",
    "expo-dev-client": "~6.0.17",
    "expo-file-system": "^19.0.17",
    "expo-font": "~14.0.8",
    "expo-image-manipulator": "^14.0.7",
    "expo-print": "~15.0.7",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "~14.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "nativewind": "^4.1.21",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-ble-plx": "^3.5.0",
    "react-native-bluetooth-classic": "^1.73.0-rc.16",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-thermal-printer": "^2.3.6",
    "react-native-thermal-receipt-printer": "^1.2.0-rc.2",
    "react-native-view-shot": "4.0.3",
    "react-native-worklets": "0.5.1",
    "with-rn-bluetooth-classic": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.10",
    "babel-preset-expo": "^54.0.6",
    "eslint": "^9.25.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.2",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.2"
  },
  "main": "node_modules/expo/AppEntry.js",
  "private": true
}
